<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="silo" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/placeable.xsd">
  <storeData>
    <name>
		<de>Mittleres universelles Unterflur-Schüttgutlager</de>
		<en>Medium MaizePlus underfloor storage system</en>
		<fr>Moyen entrepôt universel souterrain pour produits en vrac</fr>
    </name>
    <image>store/store_bulkStorage.png</image>
    <functions>
      <function>
        <de>Universelles Lager für Schüttgut.</de>
		<en>Universal storage for bulk material.</en>
		<fr>Stockage universel pour les produits en vrac.</fr>
      </function>
    </functions>
    <price>100000</price>
    <dailyUpkeep>15</dailyUpkeep>
    <lifetime>1000</lifetime>
    <rotation>0</rotation>
    <brand>NONE</brand>
    <species>placeable</species>
    <category>silos</category>
    <brush>
      <type>placeable</type>
      <category>buildings</category>
      <tab>silos</tab>
    </brush>
        <vertexBufferMemoryUsage>524032</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>68096</indexBufferMemoryUsage>
        <textureMemoryUsage>7995392</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
  </storeData>

  <base>
    <filename>i3d/undergroundStorage.i3d</filename>
  </base>

  <placement useRandomYRotation="false" useManualYRotation="true" >
    <testAreas>
      <testArea startNode="testArea1Start" endNode="testArea1End" />
    </testAreas>
	
	<sounds>
      <place template="smallImp" />
    </sounds>
  </placement>

  <clearAreas>
    <clearArea startNode="clearAreaStart" widthNode="clearAreaWidth" heightNode="clearAreaHeight"/>
  </clearAreas>

  <leveling requireLeveling="true" maxSmoothDistance="1" maxSlope="75" maxEdgeAngle="30">
    <levelAreas>
      <levelArea startNode="levelArea1Start" widthNode="levelArea1Width" heightNode="levelArea1Height"/>
    </levelAreas>
  </leveling>

  <indoorAreas>
    <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height" />
  </indoorAreas>

  <tipOcclusionUpdateAreas>
    <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
  </tipOcclusionUpdateAreas>

  <ai>
    <updateAreas>
      <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </updateAreas>
    <splines>
      <spline node="aiSpline" />
    </splines>
  </ai>

  <triggerMarkers>
    <triggerMarker node="loadingTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD" adjustToGround="true" />
    <triggerMarker node="markerUnloading" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
  </triggerMarkers>

  <infoTrigger triggerNode="infoTrigger" />

  <silo>
    <loadingStation node="loadingTrigger" supportsExtension="true" storageRadius="50">
      <loadTrigger triggerNode="loadingTrigger" fillLitersPerSecond="5000" fillTypeCategories="BULK LIQUID WINDROW MIXERWAGON FORAGEWAGON SILAGETRAILER TRAINWAGON FORAGEHARVESTER MANURESPREADER AUGERWAGON FORK SHOVEL FARMSILO HAYLOFT LOADINGVEHICLE ADDITIONAL_STORAGE_GOODS ADDITIONAL_STORAGE_BALES REVAMP_ORGANIC" aiNode="aiLoadingNode" >
		<effectNode effectClass="PipeEffect" effectNode="pipeEffect"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="0.9 0 0 0" shapeScaleSpread="0.6 0.6 1 0"/>
        <effectNode                          effectNode="pipeEffectSmoke" materialType="unloadingSmoke" fadeTime="0.5" />
      </loadTrigger>
    </loadingStation>

    <unloadingStation supportsExtension="true" storageRadius="50">
      <unloadTrigger exactFillRootNode="exactFillRootNode" fillTypeCategories="BULK LIQUID WINDROW MIXERWAGON FORAGEWAGON SILAGETRAILER TRAINWAGON FORAGEHARVESTER MANURESPREADER AUGERWAGON FORK SHOVEL FARMSILO HAYLOFT LOADINGVEHICLE ADDITIONAL_STORAGE_GOODS ADDITIONAL_STORAGE_BALES REVAMP_ORGANIC" aiNode="aiUnloadingNode"/>
    </unloadingStation>

    <storages>
      <storage node="storage" fillTypeCategories="BULK LIQUID WINDROW MIXERWAGON FORAGEWAGON SILAGETRAILER TRAINWAGON FORAGEHARVESTER MANURESPREADER AUGERWAGON FORK SHOVEL FARMSILO HAYLOFT LOADINGVEHICLE ADDITIONAL_STORAGE_GOODS ADDITIONAL_STORAGE_BALES REVAMP_ORGANIC" capacity="1000000" isExtension="false"/>
    </storages>
  </silo>

  <hotspots>
        <hotspot type="UNLOADING" linkNode="exactFillRootNode" teleportNode="mapPosition" />
   </hotspots>

  <animatedObjects>
    <animatedObject saveId="b05_Licht04" >
      <animation duration="0.5">
        <part node="3|1|1|0">
          <keyFrame time="0.00" rotation="-51 0 0" />
          <keyFrame time="1" rotation="90 0 0" />
        </part>
        <part node="2|0|0">
          <keyFrame time="0.0" visibility="false"/>
          <keyFrame time="1.0" visibility="true"/>
        </part>
      </animation>
      <controls triggerNode="3|1|0" posAction="ACTIVATE_HANDTOOL" posText="action_turnOnLight" negText="action_turnOffLight" />
    </animatedObject>
    <animatedObject saveId="p01_Switch_01" >
      <animation duration="0.5">
        <part node="3|3|1|0">
          <keyFrame time="0.00" rotation="-51 0 0" />
          <keyFrame time="1" rotation="90 0 0" />
        </part>
        <part node="5">
          <keyFrame time="0.0" visibility="false"/>
          <keyFrame time="1.0" visibility="true"/>
        </part>
      </animation>
      <controls triggerNode="3|3|0" posAction="ACTIVATE_HANDTOOL" posText="action_turnOnMarking" negText="action_turnOffMarking" />
    </animatedObject>
    <animatedObject saveId="p01_Switch_02" >
      <animation duration="0.5">
        <part node="3|4|1|0">
          <keyFrame time="0.00" rotation="-51 0 0" />
          <keyFrame time="1" rotation="90 0 0" />
        </part>
        <part node="4">
          <keyFrame time="0.0" visibility="false"/>
          <keyFrame time="1.0" visibility="true"/>
        </part>
      </animation>
      <controls triggerNode="3|4|0" posAction="ACTIVATE_HANDTOOL" posText="action_turnOnWARNING" negText="action_turnOffWARNING" />
    </animatedObject>
	<animatedObject saveId="sl01_Klappe01" >
		<animation duration="2">
			<part node="1|2|1">
				<keyFrame time="0.0" rotation="0 0 0"/>
				<keyFrame time="1" rotation="-99 0 0"/>
			</part>
			<part node="6|0|0">
				<keyFrame time="0.0" rotation="0 0 99"/>
				<keyFrame time="1" rotation="0 0 0"/>
			</part>
		</animation>
		<controls triggerNode="1|2|2" posAction="ACTIVATE_HANDTOOL" posText="action_openGate" negText="action_closeGate" />
		<sounds>
			<moving file="$data/sounds/prefab/gate/gateLoop.wav" loops="0" linkNode="1|2|1" volume="0.8" radius="25" innerRadius="3" fadeOut="0.25"/>
			<posEnd file="$data/sounds/prefab/gate/gateOpened.wav" linkNode="1|2|1" volume="0.8" radius="25" innerRadius="3" />
			<negEnd file="$data/sounds/prefab/gate/gateClosed.wav" linkNode="1|2|1" volume="0.8" radius="25" innerRadius="3" />
		</sounds>
    </animatedObject>
  </animatedObjects>

  <i3dMappings>
    <i3dMapping id="clearAreaStart" node="0>0|0|0" />
    <i3dMapping id="clearAreaWidth" node="0>0|0|0|0" />
    <i3dMapping id="clearAreaHeight" node="0>0|0|0|1" />

    <i3dMapping id="levelArea1Start" node="0>0|1|0" />
    <i3dMapping id="levelArea1Width" node="0>0|1|0|0" />
    <i3dMapping id="levelArea1Height" node="0>0|1|0|1" />

    <i3dMapping id="indoorArea1Start" node="0>0|2|0" />
    <i3dMapping id="indoorArea1Width" node="0>0|2|0|0" />
    <i3dMapping id="indoorArea1Height" node="0>0|2|0|1" />

    <i3dMapping id="testArea1Start" node="0>0|3|0" />
    <i3dMapping id="testArea1End" node="0>0|3|0|0" />

    <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>0|4|0" />
    <i3dMapping id="tipOcclusionUpdateArea1End" node="0>0|4|0|0" />
	
	<i3dMapping id="loadingTriggerMarker" node="0>7|0|4" />
	<i3dMapping id="markerUnloading" node="0>6|0|3" />
	
	<i3dMapping id="loadingTrigger" node="0>7|0|0" />
	<i3dMapping id="dischargeNode" node="0>7|0|1" />
	<i3dMapping id="pipeEffect" node="0>7|0|2|0" />
    <i3dMapping id="pipeEffectSmoke" node="0>7|0|2|1" />
	<i3dMapping id="aiLoadingNode" node="0>7|0|3" />

    <i3dMapping id="infoTrigger" node="0>9" />
	<i3dMapping id="mapPosition" node="0>8" />
	
    <i3dMapping id="exactFillRootNode" node="0>6|0|0" />
    <i3dMapping id="aiUnloadingNode" node="0>6|0|2" />
    
	<i3dMapping id="storage" node="0>" />
	<i3dMapping id="aiSpline" node="0>7|0|5" />
  </i3dMappings>
</placeable>
